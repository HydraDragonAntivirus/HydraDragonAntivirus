init("sfx", "CopyQM SFX");

function detect(bShowType, bShowVersion, bShowOptions) {
    if (MSDOS.compareEP("e9$$$$b8....8ec0a1....26a3....268c1e")) {
        if (MSDOS.compareOverlay("'TX'")) {
            sVersion = "3.24";
            sOptions = "1996 by Sydex";
            bDetected = true;
        }
    }

    return result(bShowType, bShowVersion, bShowOptions);
}