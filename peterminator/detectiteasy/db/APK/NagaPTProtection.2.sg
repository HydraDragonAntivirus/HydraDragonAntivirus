function detect(bShowType, bShowVersion, bShowOptions) {
    bDetected =
        APK.isArchiveRecordPresent("lib/armeabi/libddog.so") ||
        APK.isArchiveRecordPresent("lib/armeabi/libfdog.so");

    return result(bShowType, bShowVersion, bShowOptions);
}