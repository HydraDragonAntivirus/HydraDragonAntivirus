init("packer", "Virbox");

function detect(bShowType, bShowVersion, bShowOptions) {
    var nSection = MACH.getSectionNumber("__vdata");
    if (nSection != -1) {
        sVersion = "Virbox Protector";
        bDetected = true;
    }
    return result(bShowType, bShowVersion, bShowOptions);
}