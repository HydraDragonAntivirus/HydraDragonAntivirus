init("package", "");

function detect(bShowType, bShowVersion, bShowOptions) {
    var _sName = NPM.getPackageJsonRecord("name");

    if (_sName != "") {
        sName = _sName;
        sVersion = NPM.getPackageJsonRecord("version");
        bDetected = true;
    }

    return result(bShowType, bShowVersion, bShowOptions);
}