init("library", "IL2CPP");

function detect(bShowType, bShowVersion, bShowOptions) {
    bDetected = APK.isArchiveRecordPresent("lib/armeabi-v7a/libil2cpp.so");

    _setLang("C#", bDetected);

    return result(bShowType, bShowVersion, bShowOptions);
}