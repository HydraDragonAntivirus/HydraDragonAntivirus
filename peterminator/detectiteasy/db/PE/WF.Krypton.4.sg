init("library", "Krypton UI");

function detect(bShowType, bShowVersion, bShowOptions) {
    if (PE.isNET() && PE.isNetObjectPresent("ComponentFactory.Krypton.Toolkit")) {
        bDetected = true;
    }

    return result(bShowType, bShowVersion, bShowOptions);
}