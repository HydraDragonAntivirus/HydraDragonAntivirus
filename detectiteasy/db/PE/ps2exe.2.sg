// https://github.com/MScholtes/PS2EXE
init("packer", "PS2EXE");

function detect(bShowType, bShowVersion, bShowOptions) {
    bDetected = PE.isNetUStringPresent("^-([^: ]+)[ :]?([^:]*)$");

    _setLang("PowerShell", bDetected);
    return result(bShowType, bShowVersion, bShowOptions);
}