function detect(bShowType, bShowVersion, bShowOptions) {
    if (Binary.compare("fabe....ffe6")) {
        sVersion = "1.17cr";
        bDetected = true;
    } else if (Binary.compare("fabd....ffe5")) {
        sVersion = "1.13cs";
        bDetected = true;
    } else if (Binary.compare("fabb....ffe3")) {
        sVersion = "1.14s";
        bDetected = true;
